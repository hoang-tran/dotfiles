[color]
  ui = true
[user]
	name = Hoang Tran
	email = hoangtx.master@gmail.com
[push]
	default = upstream
[fetch]
  prune = true
[alias]
	co = checkout
  sla = log --oneline --decorate --graph --all
  sl = log --oneline --decorate --graph --all -20
  s = status
  pr = pull-request
  br = browse
  b = branch
  p = push origin HEAD
  f = fetch
  m = merge
  rb = rebase
  rbc = rebase --continue
  st = stash
  stp = stash pop
  c = commit
[core]
  editor = nvim
	excludesfile = ~/.gitignore_global
